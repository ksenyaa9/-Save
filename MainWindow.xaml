<Window x:Class="ГерасимоваГлазкиSave.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ГерасимоваГлазкиSave"
        mc:Ignorable="d"
        Title="Герасимова" Height="450" Width="800" FontFamily="Century Gothic" Icon="фото/Глазки-save.ico">
    <Grid>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>

        

        <Image Source="фото/Глазки-save.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Список агентов" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>

        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack"  HorizontalAlignment="Right"  Click="BtnBack_Click"></Button>

        


    </Grid>
</Window>
